way[power=~/line|minor_line|cable/][prop("this_voltage")>=132000]::line_9	{color: #B55D00;}
way[power=~/line|minor_line|cable/][prop("this_voltage")>=220000]::line_9	{color: #8100f9;}
way[power=~/line|minor_line|cable/][prop("this_voltage")>=310000]::line_9	{color: #6d0000;}
way[power=~/line|minor_line|cable/][prop("this_voltage")>=550000]::line_9	{color: #ff0000;}
/* busbar and bay */
way[power=line][line=busbar]
{
    casing-color:none;
    left-casing-dashes:20,20;
	left-casing-width:2;
    left-casing-color: blue;
    right-casing-dashes:20,20;
	right-casing-width:2;
    right-casing-color:blue;
}

way[power=line][line=bay]
{
    casing-color:none;
    left-casing-dashes:5,10;
	left-casing-width:2;
    left-casing-color: blue;
    right-casing-dashes:5,10;
	right-casing-width:2;
    right-casing-color:blue;
}

/* Circuit Relation Warning*/
way[power=~/line|minor_line|cable/][circuits > 1][voltage != 0][setting("circuit_relation_warning")][
	(count(parent_osm_primitives("power","circuit")) - count(parent_osm_primitives("circuits")) +
	(count(parent_osm_primitives("circuits","1")) * 1) +
	(count(parent_osm_primitives("circuits","2")) * 2) +
	(count(parent_osm_primitives("circuits","3")) * 3) +
	(count(parent_osm_primitives("circuits","4")) * 4)) != tag("circuits")
]::mult_circuits
{
    casing-dashes:8,25;
	casing-width: (((tag("circuits")-1) * 4)/2)+4 ;
    casing-color:#FF0000;
} 


/* Non-Power Lines */
/*============================================================================================*/

/* comm lines*/
way[communication=line],
way[telecom=line]
{
	casing-width:2;
    casing-opacity:0.6;
	casing-color:none;
    left-casing-dashes:10,10;
	left-casing-width:2;
	left-casing-offset:1;
    left-casing-color: pink;
    right-casing-dashes:10,10;
	right-casing-dashes-offset:10;
	right-casing-offset:1;
	right-casing-width:2;
    right-casing-color:pink;
}